{
    "name": "kostannisa-worker",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "wrangler dev",
        "deploy": "wrangler deploy",
        "migrate:local": "wrangler d1 migrations apply kost-annisa-db --local",
        "migrate:remote": "wrangler d1 migrations apply kost-annisa-db --remote",
        "seed:local": "wrangler d1 execute kost-annisa-db --local --file=./migrations/0002_seed.sql",
        "seed:remote": "wrangler d1 execute kost-annisa-db --remote --file=./migrations/0002_seed.sql",
        "typecheck": "tsc --noEmit"
    },
    "dependencies": {
        "hono": "^4.4.0",
        "zod": "^3.23.0"
    },
    "devDependencies": {
        "@cloudflare/workers-types": "^4.20240512.0",
        "@types/node": "^25.3.0",
        "typescript": "^5.5.0",
        "wrangler": "^4.67.0"
    }
}
